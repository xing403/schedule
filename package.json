    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-meta-layouts": "^0.3.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
